<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="198.0" prefWidth="335.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:controller="team.mathquest.QuitGameController">
   <children>
      <Button fx:id="saveButton" layoutX="47.0" layoutY="36.0" mnemonicParsing="false" onAction="#handleSaveButtonAction" text="I want to save my progress and quit." />
      <Button fx:id="quitButton" layoutX="30.0" layoutY="86.0" mnemonicParsing="false" onAction="#handleQuitButtonAction" text="I want to quit without saving my progress." />
      <Button fx:id="cancelButton" layoutX="44.0" layoutY="136.0" mnemonicParsing="false" onAction="#handleCancelButtonAction" text="I made a mistake. I don't want to quit." />
   </children>
</AnchorPane>
